{
    "template": "post_macros",
    "description": "Adds bookmark link to posts",
    "execution_order": 10,
    "enabled": true,
    "action": "preg_replace",
    "find": "/<xf:if is=\"\\$post\\.isUnread(.*?)<\\/xf:if>/s",
    "replace": "$0\n<xf:include template=\"th_post_macros_bookmarkLink_bookmarks\" />"
}